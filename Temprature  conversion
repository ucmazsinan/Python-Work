def Fahrenheit():
    F = 0 # initial Fahrenheit temprature
    
    for celcius in range(0,101): # Celcius temprature throught 1 to 100
        F = round(((9 / 5) * celcius) + 32, 1) # Calculate Fahrenheit temprature each loop iteration
        print(f"{celcius : >4}{F : >13}") # print celcius to fahreint each loop print top to bottom
        
    return F # Return F calculate each loop iteration 

print(f'Celcius{"Fahrenheit" : >13}') # print like tabular format 
Fahrenheit() # call function
