def display_table(element):
    print(f"{'Element İndex'}{'Elements':>9}")
    for index, row in enumerate(element):
        for column,element in enumerate(row):
            print(f'element[{index}][{column}] = {element}')
            
 TwoD = [[1, 2, 3, 5],
        [5, 6, 7, 8]]
 
 display_table(TwoD)
 
Element İndex Elements
element[0][0] = 1
element[0][1] = 2
element[0][2] = 3
element[0][3] = 5
element[1][0] = 5
element[1][1] = 6
element[1][2] = 7
element[1][3] = 8
